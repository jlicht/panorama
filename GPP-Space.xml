<krpano>

	<control mousetype="drag2d" touchtype="drag2d" zoomtocursor="true" zoomoutcursor="true" bouncinglimits="true" keycodesin="16" keycodesout="17" />

	<!-- the skin -->

	<!-- view settings -->
	<view hlookat="0" vlookat="0" maxpixelzoom="1.0" fovmax="150" limitview="range" vlookatmin="-53.847" vlookatmax="53.847" />


	<preview url="GPP-Space.tiles/preview.jpg" />

	<image>
